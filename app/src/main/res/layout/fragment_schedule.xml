<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              tools:context="com.anrongtec.laucher.ui.fragment.ScheduleFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/tv_main_search"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="@drawable/shape_bg_main_search"
        android:drawableLeft="@drawable/img_main_search"
        android:drawablePadding="@dimen/m6"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/m10"
        android:text="限搜索身份证号，车牌号，地点"
        android:textColor="#C5C8C8"
        android:textSize="@dimen/t12"
        android:visibility="gone"/>

    <com.anrongtec.laucher.webview.SwipeRefreshWebView
        android:id="@+id/web_schedule"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <ImageView
        android:id="@+id/iv_go_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/m5"
        android:src="@drawable/icon_go_back"
        android:visibility="gone"
        />

</RelativeLayout>
